    <DataSet Name="gInstanceTypeSummary">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>SELECT SI.SQLInstanceType, COUNT(*) AS SQLInstanceCount
  FROM dbo.SQLClusters SC
  JOIN dbo.SQLInstances SI
    ON SC.SQLClusterID = SI.SQLClusterID
 WHERE SI.SQLInstanceType = 'Microsoft Clustering'
    OR SI.SQLInstanceType = 'Veritas Clustering'
GROUP BY SI.SQLInstanceType

UNION ALL

SELECT 'Microsoft Clustering', 0

UNION ALL

SELECT 'Veritas Clustering', 0 

UNION ALL

SELECT 'AlwaysOn Availiability Group', COUNT(DISTINCT SI.SQLInstanceID) AS ObjCount
  FROM dbo.SQLInstances SI
  JOIN dbo.AGInstances AGI
   ON SI.SQLInstanceID=AGi.SQLInstanceID

UNION ALL 

SELECT 'Stand Alone', COUNT(*)
  FROM dbo.SQLInstances SI
 WHERE SI.SQLInstanceType &lt;&gt; 'Microsoft Clustering'
   AND SI.SQLInstanceType &lt;&gt; 'Veritas Clustering'
   AND SI.SQLInstanceID NOT IN (SELECT SQLInstanceID FROM dbo.AGInstances)</CommandText>
      </Query>
      <Fields>
        <Field Name="SQLInstanceType">
          <DataField>SQLInstanceType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SQLInstanceCount">
          <DataField>SQLInstanceCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
