  <DataSets>
    <DataSet Name="gSQLInstanceSummary">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>SELECT SI.SQLInstanceEnviornment, SV.SQLVersionShortName AS SQLVersion
  FROM dbo.vSQLInstances SI
  JOIN dbo.SQLVersions SV
    ON SI.SQLInstanceVersionID = SV.SQLVersionID
 WHERE SI.LastUpdated &gt;= DATEADD(DAY,-1,GETDATE())</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="SQLInstanceEnviornment">
          <DataField>SQLInstanceEnviornment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SQLVersion">
          <DataField>SQLVersion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!SQLVersion.Value</FilterExpression>
          <Operator>NotEqual</Operator>
          <FilterValues>
            <FilterValue>="Unknown"</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
    <DataSet Name="report_InstanceCountBySupportDate">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>; WITH cte AS (SELECT     SV.SQLVersion, SV.SQLVersionSupportEndDate, SI.SQLInstanceVersionID, (SV.SQLMajorVersion * 100) 
                                                      + SV.SQLMinorVersion AS SQLMajorMinorVersion, (SV.SQLMajorVersion * 1000000) + (SV.SQLMinorVersion * 10000) + SV.SQLBuild AS SortKey
                                FROM         dbo.SQLInstances SI JOIN
                                                      dbo.Servers S ON SI.ServerID = S.ServerID AND SI.SQLClusterID IS NULL JOIN
                                                      dbo.SQLVersions SV ON SI.SQLInstanceVersionID = SV.SQLVersionID
                                WHERE     SV.SQLVersion &lt;&gt; 'Unknown'
                                UNION ALL
                                SELECT     SV.SQLVersion, SV.SQLVersionSupportEndDate, SI.SQLInstanceID, (SV.SQLMajorVersion * 100) + SV.SQLMinorVersion AS SQLMajorMinorVersion, 
                                                      (SV.SQLMajorVersion * 1000000) + (SV.SQLMinorVersion * 10000) + SV.SQLBuild
                                FROM         dbo.SQLInstances SI JOIN
                                                      dbo.SQLClusters SC ON SI.SQLClusterID = SC.SQLClusterID AND SI.ServerID IS NULL JOIN
                                                      dbo.SQLClusterNodes SCN ON SC.SQLClusterID = SCN.SQLClusterID AND SCN.IsActiveNode = 1 JOIN
                                                      dbo.Servers S ON SCN.SQLNodeID = S.ServerID JOIN
                                                      dbo.SQLVersions SV ON SI.SQLInstanceVersionID = SV.SQLVersionID
                                WHERE     SV.SQLVersion &lt;&gt; 'Unknown'), cte2 AS
    (SELECT     SQLVersion, SQLMajorMinorVersion, SortKey, MAX(SQLVersionSupportEndDate) AS SupportEndDate, COUNT(*) AS InstCount
      FROM          cte
      GROUP BY SQLVersion, SQLMajorMinorVersion, SortKey), cte3 AS
    (SELECT     (SV.SQLMajorVersion * 100) + SV.SQLMinorVersion AS SQLMajorMinorVersion, MAX(SQLVersionSupportEndDate) AS SQLVersionSupportEndDate
      FROM          dbo.SQLVersions AS SV
      GROUP BY (SV.SQLMajorVersion * 100) + SV.SQLMinorVersion)
    SELECT     cte2.SQLVersion, cte2.InstCount, cte2.SupportEndDate AS BuildSupportEndDate, cte3.SQLVersionSupportEndDate, cte2.SortKey
     FROM         cte2 JOIN
                            cte3 ON cte2.SQLMajorMinorVersion = cte3.SQLMajorMinorVersion</CommandText>
      </Query>
      <Fields>
        <Field Name="SQLVersion">
          <DataField>SQLVersion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BuildSupportEndDate">
          <DataField>BuildSupportEndDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="InstCount">
          <DataField>InstCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SQLVersionSupportEndDate">
          <DataField>SQLVersionSupportEndDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="SortKey">
          <DataField>SortKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!BuildSupportEndDate.Value</FilterExpression>
          <Operator>LessThanOrEqual</Operator>
          <FilterValues>
            <FilterValue>=Now()</FilterValue>
          </FilterValues>
        </Filter>
        <Filter>
          <FilterExpression>=Fields!SQLVersionSupportEndDate.Value</FilterExpression>
          <Operator>LessThanOrEqual</Operator>
          <FilterValues>
            <FilterValue>=Now()</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
    <DataSet Name="gDatabaseSummary">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>WITH AGInstancesByRowNumber AS (
 SELECT AGI.AGID,
        AGI.SQLInstanceID,
        ROW_NUMBER () OVER (PARTITION BY AGI.AGID ORDER BY AGI.SQLInstanceID) AS InstanceNumber
   FROM dbo.AGInstances AGI)
, DatabaseCounts AS (
   SELECT D.SQLInstanceID,
          D.DatabaseState,
          COUNT(*) DBCount
     FROM dbo.Databases D
	 WHERE LastUpdated &gt;= DATEADD(DAY,-1,GETDATE())
     and D.DatabaseID NOT IN (SELECT AGD.DatabaseID
  FROM AGInstancesByRowNumber AGI2
  JOIN dbo.AGInstances AGI
    ON AGI2.SQLInstanceID = AGI.SQLInstanceID
   AND AGI2.AGID = AGI.AGID
   AND AGI2.InstanceNumber &gt;  1
  JOIN dbo.AGDatabases AGD
    ON AGI.AGInstanceID = AGD.AGInstanceID)
 GROUP BY D.SQLInstanceID, D.DatabaseState)
   SELECT SI.SQLInstanceEnviornment, REPLACE(REPLACE(REPLACE(SI.SQLInstanceEdition,' (64-bit)',''),': Core-based Licensing',''),' with Advanced Services','') AS SQLEdition, SV.SQLVersionShortName AS SQLVersion, DC.DatabaseState, SUM(DC.DBCount) AS DBCount
     FROM dbo.SQLInstances SI
LEFT JOIN DatabaseCounts DC
       ON DC.SQLInstanceID = SI.SQLInstanceID
LEFT JOIN dbo.SQLVersions SV
       ON SI.SQLInstanceVersionID = SV.SQLVersionID
    WHERE DBCount IS NOT NULL
	  AND DatabaseState = 'Online'
	  AND SI.SQLInstanceEdition NOT LIKE 'Express Edition%'
                    AND SI.LastUpdated &gt;= DATEADD(DAY,-1,GETDATE())
 GROUP BY SI.SQLInstanceEnviornment, SI.SQLInstanceEdition, SV.SQLVersionShortName, DC.DatabaseState
 ORDER BY SQLVersionShortName, SQLInstanceEnviornment</CommandText>
      </Query>
      <Fields>
        <Field Name="SQLInstanceEnviornment">
          <DataField>SQLInstanceEnviornment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SQLEdition">
          <DataField>SQLEdition</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SQLVersion">
          <DataField>SQLVersion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DBCount">
          <DataField>DBCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DatabaseState">
          <DataField>DatabaseState</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="gDatabaseSizeSummary">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>WITH AGInstancesByRowNumber AS (
 SELECT AGI.AGID,
        AGI.SQLInstanceID,
        ROW_NUMBER () OVER (PARTITION BY AGI.AGID ORDER BY AGI.SQLInstanceID) AS InstanceNumber
   FROM dbo.AGInstances AGI)
, DatabaseSizes AS (
   SELECT D.SQLInstanceID, D.DatabaseID, SUM(DS.FileSize_mb) AS DBSize_mb
     FROM dbo.Databases D
     JOIN dbo.DatabaseSize DS
       ON D.DatabaseID = DS.DatabaseID
    WHERE 
		d.LastUpdated &gt;= DATEADD(DAY,-1,GETDATE()) 
		AND 
		D.DatabaseID NOT IN 
		 (SELECT AGD.DatabaseID
			FROM AGInstancesByRowNumber AGI2
			JOIN dbo.AGInstances AGI
			ON AGI2.SQLInstanceID = AGI.SQLInstanceID
			AND AGI2.AGID = AGI.AGID
			AND AGI2.InstanceNumber &gt;  1
			JOIN dbo.AGDatabases AGD
			ON AGI.AGInstanceID = AGD.AGInstanceID)
       AND DS.DateCaptured = CAST(GETDATE() AS DATE)
 GROUP BY D.SQLInstanceID, D.DatabaseID)
   SELECT SI.SQLInstanceEnviornment, SV.SQLVersionShortName AS SQLVersion, ISNULL(SUM(DBSize_mb)/1024./1024.,0) AS DBSize_tb
     FROM dbo.SQLInstances SI
LEFT JOIN DatabaseSizes DS
       ON DS.SQLInstanceID = SI.SQLInstanceID
LEFT JOIN dbo.SQLVersions SV
       ON SI.SQLInstanceVersionID = SV.SQLVersionID
    WHERE DBSize_mb IS NOT NULL
  AND SI.SQLInstanceEdition NOT LIKE 'Express Edition%'
  AND SI.LastUpdated &gt;= DATEADD(DAY,-1,GETDATE())
 GROUP BY SI.SQLInstanceEnviornment, SV.SQLVersionShortName
 ORDER BY SQLVersion, SQLInstanceEnviornment</CommandText>
      </Query>
      <Fields>
        <Field Name="SQLInstanceEnviornment">
          <DataField>SQLInstanceEnviornment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SQLVersion">
          <DataField>SQLVersion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DBSize_tb">
          <DataField>DBSize_tb</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
