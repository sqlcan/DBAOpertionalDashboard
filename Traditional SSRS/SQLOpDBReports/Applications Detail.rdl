    <DataSet Name="ApplicationDetails">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ApplicationName">
            <Value>=Parameters!ApplicationName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH CTE AS (SELECT DatabaseID, MAX(DateCaptured) AS LastDate FROM dbo.DatabaseSize GROUP BY DatabaseID)
SELECT SI.ServerInstance, SI.SQLInstanceEnviornment, App.ApplicationName, App.ApplicationOwner, D.DatabaseName, SUM(DS.FileSize_mb) AS TotalDBSize_mb
  FROM dbo.Application App
  JOIN dbo.Databases D
    ON App.ApplicationID = D.ApplicationID
  JOIN dbo.vSQLInstances SI
    ON D.SQLInstanceID = SI.SQLInstanceID
  JOIN dbo.DatabaseSize DS
    ON DS.DatabaseID = D.DatabaseID
  JOIN CTE C
    ON DS.DatabaseID = C.DatabaseID
   AND DS.DateCaptured = C.LastDate
WHERE ApplicationName = @ApplicationName
GROUP BY SI.ServerInstance, SI.SQLInstanceEnviornment, App.ApplicationName, App.ApplicationOwner, D.DatabaseName</CommandText>
      </Query>
      <Fields>
        <Field Name="ServerInstance">
          <DataField>ServerInstance</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SQLInstanceEnviornment">
          <DataField>SQLInstanceEnviornment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ApplicationName">
          <DataField>ApplicationName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ApplicationOwner">
