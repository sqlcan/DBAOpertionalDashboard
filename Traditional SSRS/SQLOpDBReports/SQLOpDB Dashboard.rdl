  <DataSets>
    <DataSet Name="SQLServicesOverview">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText> SELECT ServiceType, Status, SUM(SvcCount) As SvcCount
   FROM Snapshot.SQLServices SC
  WHERE ((@Status = 'Healthy') and (Status = 'Running'))
     OR ((@Status = 'Unhealthy') and (Status &lt;&gt; 'Running'))
	 OR ((@Status = 'Both'))
GROUP BY ServiceType, Status 
order by ServiceType</CommandText>
      </Query>
      <Fields>
        <Field Name="ServiceType">
          <DataField>ServiceType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SvcCount">
          <DataField>SvcCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="SQLErrorsOverview">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT *
 FROM Snapshot.SQLErrors
WHERE ((@Status='Healthy') AND (ErrorStatus = 'No Errors'))
OR  ((@Status='Unhealthy') AND (ErrorStatus = 'Errors'))
OR @Status = 'Both'</CommandText>
      </Query>
