    <DataSet Name="DatabaseOverview">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DatabaseState, RwCount
  FROM Snapshot.Databases DB
WHERE (((@Status = 'Healthy') AND (DatabaseState = 'Online'))
OR ((@Status = 'Unhealthy') AND (DatabaseState &lt;&gt; 'Online')) 
OR @Status = 'Both')</CommandText>
      </Query>
      <Fields>
        <Field Name="DatabaseState">
          <DataField>DatabaseState</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RwCount">
