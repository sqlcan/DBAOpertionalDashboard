  <DataSets>
    <DataSet Name="dsServerTrends">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>  SELECT OS.OperatingSystemName, S.YearMonth, S.ServerCount
    FROM Trending.Servers S
    JOIN dbo.OperatingSystems OS
      ON S.OperatingSystemID = OS.OperatingSystemID
   WHERE OS.OperatingSystemName &lt;&gt; 'Unknown'
     AND S.YearMonth &gt;= CAST(CONVERT(VARCHAR(6),DATEADD(Month,-12,GETDATE()),112) AS INT)
ORDER BY  OS.OperatingSystemName, S.YearMonth</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="OperatingSystemName">
          <DataField>OperatingSystemName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="YearMonth">
          <DataField>YearMonth</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ServerCount">
          <DataField>ServerCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsSQLInstanceTrends">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>WITH SQLVersionTrimmed AS
(
    SELECT CASE WHEN SV.SQLVersion &lt;&gt; 'Unknown' THEN
            CASE WHEN CHARINDEX(' Service Pack',SV.SQLVersion) = 0 THEN
			    REPLACE(SUBSTRING(SV.SQLVersion,1,CHARINDEX(' RTM',SV.SQLVersion)),'Microsoft ','')
		    ELSE
			    REPLACE(SUBSTRING(SV.SQLVersion,1,CHARINDEX(' Service Pack',SV.SQLVersion)),'Microsoft ','')
		    END
		    ELSE
		    SV.SQLVersion
		    END AS SQLVersion,
            SV.SQLVersionID
            FROM dbo.SQLVersions SV)
  SELECT SV.SQLVersion, SI.Environment, SI.YearMonth, SUM(SI.InstanceCount)  AS InstanceCount
    FROM Trending.SQLInstances SI
    JOIN SQLVersionTrimmed SV
      ON SI.ServerVersionID = SV.SQLVersionID
   WHERE SV.SQLVersion &lt;&gt; 'Unknown'
     AND SI.YearMonth &gt;= CAST(CONVERT(VARCHAR(6),DATEADD(Month,-12,GETDATE()),112) AS INT)
GROUP BY SV.SQLVersion, SI.Environment, SI.YearMonth
ORDER BY  SI.Environment,SV.SQLVersion, SI.YearMonth</CommandText>
      </Query>
      <Fields>
        <Field Name="SQLVersion">
          <DataField>SQLVersion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Environment">
          <DataField>Environment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="YearMonth">
          <DataField>YearMonth</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="InstanceCount">
          <DataField>InstanceCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsDatabaseTrends">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>WITH SQLVersionTrimmed AS
(
    SELECT CASE WHEN SV.SQLVersion &lt;&gt; 'Unknown' THEN
            CASE WHEN CHARINDEX(' Service Pack',SV.SQLVersion) = 0 THEN
			    REPLACE(SUBSTRING(SV.SQLVersion,1,CHARINDEX(' RTM',SV.SQLVersion)),'Microsoft ','')
		    ELSE
			    REPLACE(SUBSTRING(SV.SQLVersion,1,CHARINDEX(' Service Pack',SV.SQLVersion)),'Microsoft ','')
		    END
		    ELSE
		    SV.SQLVersion
		    END AS SQLVersion,
            SV.SQLVersionID
            FROM dbo.SQLVersions SV)
  SELECT SV.SQLVersion, D.Environment, D.YearMonth, SUM(D.DatabaseCount)  AS DatabaseCount, SUM(D.DatabaseTotalSize) AS DatabaseTotalSize_mb
    FROM Trending.Databases D
    JOIN SQLVersionTrimmed SV
      ON D.SQLVersionID = SV.SQLVersionID
   WHERE SV.SQLVersion &lt;&gt; 'Unknown'
     AND D.YearMonth &gt;= CAST(CONVERT(VARCHAR(6),DATEADD(Month,-12,GETDATE()),112) AS INT)
GROUP BY SV.SQLVersion, D.Environment, D.YearMonth
ORDER BY  D.Environment,SV.SQLVersion, D.YearMonth</CommandText>
      </Query>
      <Fields>
        <Field Name="SQLVersion">
          <DataField>SQLVersion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Environment">
          <DataField>Environment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="YearMonth">
          <DataField>YearMonth</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DatabaseCount">
          <DataField>DatabaseCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DatabaseTotalSize_mb">
          <DataField>DatabaseTotalSize_mb</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
