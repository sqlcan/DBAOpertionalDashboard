        </Field>
        <Field Name="DBSize_gb">
          <DataField>DBSize_gb</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>WITH CTE AS (SELECT DatabaseID, SUM(FileSize_mb) AS DBSize_mb
  FROM dbo.DatabaseSize DS
 WHERE DS.DateCaptured = CAST(GETDATE() AS DATE)
GROUP BY DatabaseID),
CTE2 AS (SELECT SV.SQLVersionID, CASE WHEN SV.SQLVersion &lt;&gt; 'Unknown' THEN
	        CASE WHEN CHARINDEX(' Service Pack',SV.SQLVersion) = 0 THEN
			   REPLACE(SUBSTRING(SV.SQLVersion,1,CHARINDEX(' RTM',SV.SQLVersion)),'Microsoft SQL Server ','')
		    ELSE
			   REPLACE(SUBSTRING(SV.SQLVersion,1,CHARINDEX(' Service Pack',SV.SQLVersion)),'Microsoft SQL Server ','')
		    END
		 ELSE
		    SV.SQLVersion
		 END AS SQLVersion
		 FROM dbo.SQLVersions SV)
SELECT CASE WHEN S.ServerName IS NULL THEN SC.SQLClusterName ELSE S.ServerName END AS ServerVCOName,
       SI.SQLInstanceName, SI.SQLInstanceEnviornment, SV.SQLVersion, D.DatabaseName, ISNULL(DBSize_mb/1024.,0) AS DBSize_gb
  FROM dbo.SQLInstances SI
  LEFT JOIN dbo.Databases D
    ON SI.SQLInstanceID = D.SQLInstanceID
  LEFT JOIN CTE DS
    ON D.DatabaseID = DS.DatabaseID
  LEFT JOIN CTE2 SV
    ON SI.SQLInstanceVersionID = SV.SQLVersionID
  LEFT JOIN dbo.Servers S
    ON SI.ServerID = S.ServerID
   AND SI.SQLClusterID IS NULL
  LEFT JOIN dbo.SQLClusters SC
    ON SI.SQLClusterID = SC.SQLClusterID
   AND SI.ServerID IS NULL
 WHERE DatabaseName IS NOT NULL
      AND SV.SQLVersion &lt;&gt; '2000'
ORDER BY SQLVersion, SQLInstanceEnviornment</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
