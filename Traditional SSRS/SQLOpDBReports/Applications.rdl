    <DataSet Name="ApplicationList">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>  SELECT App.ApplicationID, App.ApplicationName, App.ApplicationOwner, SI.SQLInstanceEnviornment, COUNT(*) AS DatabaseCount
    FROM dbo.Application App
    JOIN dbo.Databases D
      ON D.ApplicationID = App.ApplicationID
    JOIN dbo.vSQLInstances SI
      ON D.SQLInstanceID = SI.SQLInstanceID
GROUP BY App.ApplicationID, App.ApplicationName, App.ApplicationOwner, SI.SQLInstanceEnviornment</CommandText>
      </Query>
      <Fields>
        <Field Name="ApplicationID">
          <DataField>ApplicationID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ApplicationName">
          <DataField>ApplicationName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ApplicationOwner">
          <DataField>ApplicationOwner</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SQLInstanceEnviornment">
          <DataField>SQLInstanceEnviornment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DatabaseCount">
          <DataField>DatabaseCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
