    <DataSet Name="ExtendedPropertiesByInstance">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ExtendedPropertyID">
            <Value>=Parameters!ExtendedPropertyID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SQLInstanceID">
            <Value>=Parameters!SQLInstanceID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT EPV.SQLInstanceID, SI.ServerInstance, EP.ExtendedPropertyID, EP.ExtendedPropertyName, EPV.ExtendedPropertyValue, EPV.DiscoveryOn, EPV.LastUpdated
  FROM dbo.ExtendedProperty EP
  JOIN dbo.ExtendedPropertyValues EPV
    ON EP.ExtendedPropertyID = EPV.ExtendedPropertyID
  JOIN dbo.vSQLInstances SI
    ON EPV.SQLInstanceID = SI.SQLInstanceID
WHERE ((EP.ExtendedPropertyID = @ExtendedPropertyID) OR (@ExtendedPropertyID IS NULL))
AND ((@SQLInstanceID IS NULL) OR (SI.SQLInstanceID = @SQLInstanceID))</CommandText>
      </Query>
      <Fields>
        <Field Name="SQLInstanceID">
          <DataField>SQLInstanceID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ServerInstance">
