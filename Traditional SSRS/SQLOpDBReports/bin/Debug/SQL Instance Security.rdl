  <DataSets>
    <DataSet Name="ServerMembershipsAndLogins">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@LoginName">
            <Value>=Parameters!LoginName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SQLInstanceID">
            <Value>=Parameters!SQLInstanceID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>   SELECT SI.ServerInstance, ServerRole.PrincipalName AS ServerRoleName, ServerLogin.PrincipalName AS ServerLoginName, ServerLogin.PrincipalType AS ServerLoginType, SPM.IsArchived, SPM.DiscoveryOn, SPM.LastUpdated
     FROM Security.ServerPrincipalMembership SPM
LEFT JOIN Security.ServerPrincipal ServerRole
       ON SPM.ServerRoleID = ServerRole.PrincipalID
LEFT JOIN Security.ServerPrincipal ServerLogin
       ON SPM.ServerLoginID = ServerLogin.PrincipalID
LEFT JOIN dbo.vSQLInstances SI
       ON SPM.SQLInstanceID = SI.SQLInstanceID
WHERE ((@LoginName IS NULL) OR (ServerLogin.PrincipalName LIKE @LoginName))
AND ((SI.SQLInstanceID = @SQLInstanceID) OR (@SQLInstanceID = 0))
AND (((SPM.IsArchived = 1) AND (@Status = 'Audit')) OR
          ((SPM.IsArchived = 0) AND (@Status = 'Active')) OR
          (@Status = 'Both'))
AND ServerLogin.PrincipalName NOT IN ('sa')</CommandText>
      </Query>
      <Fields>
        <Field Name="ServerInstance">
          <DataField>ServerInstance</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ServerRoleName">
          <DataField>ServerRoleName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ServerLoginName">
          <DataField>ServerLoginName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ServerLoginType">
          <DataField>ServerLoginType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IsArchived">
          <DataField>IsArchived</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="DiscoveryOn">
          <DataField>DiscoveryOn</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="LastUpdated">
          <DataField>LastUpdated</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DatabaseExplicitPermissions">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@LoginName">
            <Value>=Parameters!LoginName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SQLInstanceID">
            <Value>=Parameters!SQLInstanceID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>   SELECT SI.ServerInstance, Grantee.PrincipalName AS DatabaseUserName, Grantee.PrincipalName AS GranteeName, SP.ObjectID, SP.ObjectType, SP.Access, SP.PermissionName, SP.IsArchived, SP.DiscoveryOn, SP.LastUpdated
     FROM Security.ServerPermission SP
LEFT JOIN Security.ServerPrincipal Grantor
       ON SP.GrantorID = Grantor.PrincipalID
LEFT JOIN Security.ServerPrincipal Grantee
       ON SP.GranteeID = Grantee.PrincipalID
LEFT JOIN dbo.vSQLInstances SI
       ON SP.SQLInstanceID = SI.SQLInstanceID
    WHERE ((@LoginName IS NULL) OR (Grantee.PrincipalName LIKE @LoginName))
AND ((SI.SQLInstanceID = @SQLInstanceID) OR (@SQLInstanceID = 0))
AND (((SP.IsArchived = 1) AND (@Status = 'Audit')) OR
          ((SP.IsArchived = 0) AND (@Status = 'Active')) OR
          (@Status = 'Both'))</CommandText>
      </Query>
      <Fields>
        <Field Name="ServerInstance">
          <DataField>ServerInstance</DataField>
          <rd:TypeName>System.String</rd:TypeName>
