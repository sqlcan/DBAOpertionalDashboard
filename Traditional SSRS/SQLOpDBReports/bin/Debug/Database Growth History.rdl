  <DataSets>
    <DataSet Name="GrowthHistory_Daily">
      <Query>
        <DataSourceName>PrimaryDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DatabaseID">
            <Value>=Parameters!DatabaseID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH CTE AS (
  SELECT DatabaseID, DateCaptured, FileType, FileSize_mb, ROW_NUMBER() OVER (PARTITION BY FileType ORDER BY DatabaseID ASC, DateCaptured ASC) AS RowNum
    FROM dbo.DatabaseSize
   WHERE DatabaseID = @DatabaseID
),
CTE2 AS (
SELECT DBS1.DatabaseID, DBS1.DateCaptured, DBS1.FileType,
       CAST(ROUND((1-((DBS2.FileSize_mb*1.0)/DBS1.FileSize_mb)),4) AS decimal(7,4)) AS File_GrowthInPercent
  FROM CTE DBS1 
  JOIN CTE DBS2
    ON DBS1.RowNum = DBS2.RowNum+1
   AND DBS1.DatabaseID = DBS2.DatabaseID
   AND DBS1.FileType = DBS2.FileType)
SELECT D.DatabaseID,
	   DS.DateCaptured,
	   DS.FileType,
	   DS.FileSize_mb,
	   ISNULL(CTE2.File_GrowthInPercent,0) AS GrowthInPercent
  FROM dbo.Databases D
  JOIN dbo.DatabaseSize DS
    ON D.DatabaseID = DS.DatabaseID
  LEFT JOIN CTE2 
    ON DS.DatabaseID = CTE2.DatabaseID
   AND DS.DateCaptured = CTE2.DateCaptured
   AND DS.FileType = CTE2.FileType
WHERE D.DatabaseID = @DatabaseID
ORDER BY DS.DateCaptured</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DatabaseID">
          <DataField>DatabaseID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FileSize_mb">
          <DataField>FileSize_mb</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="DateCaptured">
          <DataField>DateCaptured</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="FileType">
          <DataField>FileType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="GrowthInPercent">
          <DataField>GrowthInPercent</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="GrowthHistory_Monthly">
      <Query>
        <DataSourceName>PrimaryDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DatabaseID">
            <Value>=Parameters!DatabaseID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH CTE AS (
   SELECT DatabaseID, YearMonth, FileType, FileSize_mb, ROW_NUMBER() OVER (PARTITION BY FileType ORDER BY DatabaseID ASC, YearMonth ASC) AS RowNum
     FROM History.DatabaseSize
    WHERE DatabaseID = @DatabaseID),
CTE2 AS (
   SELECT DBS1.DatabaseID, DBS1.YearMonth, DBS1.FileType,
          CAST(ROUND((1 - ((DBS2.FileSize_mb * 1.0) / DBS1.FileSize_mb)), 4) AS decimal(7, 4)) AS File_GrowthInPercent
     FROM CTE DBS1
	 JOIN CTE DBS2
	   ON DBS1.RowNum = DBS2.RowNum + 1
	  AND DBS1.DatabaseID = DBS2.DatabaseID
	  AND DBS1.FileType = DBS2.FileType)
   SELECT DS.YearMonth, DS.FileType, FileSize_mb, File_GrowthInPercent AS GrowthInPercent
     FROM dbo.Databases D
     JOIN History.DatabaseSize DS
       ON D .DatabaseID = DS.DatabaseID 
LEFT JOIN CTE2
       ON DS.DatabaseID = CTE2.DatabaseID
	  AND DS.YearMonth = CTE2.YearMonth
	  AND DS.FileType = CTE2.FileType
    WHERE D .DatabaseID = @DatabaseID
 ORDER BY DS.YearMonth</CommandText>
      </Query>
      <Fields>
        <Field Name="FileSize_mb">
          <DataField>FileSize_mb</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="YearMonth">
          <DataField>YearMonth</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FileType">
          <DataField>FileType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="GrowthInPercent">
          <DataField>GrowthInPercent</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
