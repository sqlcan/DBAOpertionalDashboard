    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DatabaseList">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DatabaseState">
            <Value>=Parameters!DatabaseState.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SQLVersion">
            <Value>=Parameters!SQLVersion.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SQLInstanceEnviornment">
            <Value>=Parameters!SQLInstanceEnviornment.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SQLInstanceID">
            <Value>=Parameters!SQLInstanceID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RptCaller">
            <Value>=Parameters!RptCaller.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH DatabaseSizeLastCollection AS (
  SELECT DatabaseID,
         FileType,
         MAX(DatabaseSizeID) AS LastCollectionID
    FROM dbo.DatabaseSize DS
GROUP BY DatabaseID, FileType),
DatabaseSize_mb AS (
SELECT DS.DatabaseID, DS.DateCaptured, DS.FileType, DS.FileSize_mb
  FROM dbo.DatabaseSize DS
  JOIN DatabaseSizeLastCollection DSL
    ON DS.DatabaseID = DSL.DatabaseID
   AND DS.DatabaseSizeID = DSL.LastCollectionID
   AND DS.FileType = DSL.FileType)
SELECT   SI.ServerInstance
          , SI.SQLInstanceEnviornment
          , SV.SQLVersionShortName AS SQLVersion
          , SI.SQLInstanceID
		  , App.ApplicationID
		  , App.ApplicationName
          , D.DatabaseID
          , D.DatabaseName
          , D.DatabaseState
          , ISNULL(DSd.FileSize_mb,0) AS DataFileSize_mb
          , ISNULL(DSl.FileSize_mb,0) AS LogFileSize_mb
		  , AG.AGID
		  , AG.AGName AS AvailabilityGroupName
          , D.DiscoveryOn
          , D.LastUpdated
  FROM dbo.vSQLInstances SI
  JOIN dbo.Databases D
    ON SI.SQLInstanceID = D.SQLInstanceID
LEFT JOIN DatabaseSize_mb DSd
    ON D.DatabaseID  = DSd.DatabaseID
   AND DSd.FileType = 'Data'
LEFT  JOIN DatabaseSize_mb DSl
    ON D.DatabaseID  = DSl.DatabaseID
   AND DSl.FileType = 'Log'
  JOIN dbo.SQLVersions SV
    ON SI.SQLInstanceVersionID = SV.SQLVersionID
LEFT JOIN dbo.Application App
    ON D.ApplicationID = App.ApplicationID
LEFT JOIN dbo.AGDatabases AGD
    ON AGD.DatabaseID = D.DatabaseID
LEFT JOIN dbo.AGInstances AGI
    ON AGI.AGInstanceID = AGD.AGInstanceID
LEFT JOIN dbo.AGs AG
    ON AGI.AGID = AG.AGID
WHERE ((D.DatabaseState = @DatabaseState) OR (@DatabaseState = 'ALL'))
   AND ((SV.SQLVersionShortName = @SQLVersion) OR (@SQLVersion = 'ALL'))
   AND ((SI.SQLInstanceEnviornment = @SQLInstanceEnviornment) OR (@SQLInstanceEnviornment = 'ALL'))
   AND ((D.SQLInstanceID = @SQLInstanceID) OR (@SQLInstanceID = -1))
AND ((@RptCaller IS NULL) OR ((@RptCaller = 'SQLOpDB') AND (D.LastUpdated &gt;= CAST(DATEADD(DAY,-1,GETDATE()) AS DATE))))
AND  NOT EXISTS (SELECT *
                     FROM dbo.Databases_Exclusions DE
                    WHERE D.DatabaseID = DE.DatabaseID
                      AND SI.SQLInstanceID = DE.SQLInstanceID
                      AND D.DatabaseName = DE.DatabaseName)</CommandText>
      </Query>
