  <DataSets>
    <DataSet Name="DatabaseMembershipsAndUsers">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DatabaseID">
            <Value>=Parameters!DatabaseID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserName">
            <Value>=Parameters!UserName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SQLInstanceID">
            <Value>=Parameters!SQLInstanceID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>   SELECT SI.ServerInstance, D.DatabaseName, DBRole.PrincipalName AS DatabaseRoleName, DBUser.PrincipalName AS DatabaseUserName, DBUser.PrincipalType AS DatabaseUserType, DPM.IsOrphaned, DPM.IsArchived, DPM.DiscoveryOn, DPM.LastUpdated
     FROM Security.DatabasePrincipalMembership DPM
LEFT JOIN Security.DatabasePrincipal DBRole
       ON DPM.DatabaseRoleID = DBRole.PrincipalID
LEFT JOIN Security.DatabasePrincipal DBUser
       ON DPM.DatabaseUserID = DBUser.PrincipalID
LEFT JOIN dbo.Databases D
       ON DPM.DatabaseID = D.DatabaseID
LEFT JOIN dbo.vSQLInstances SI
       ON D.SQLInstanceID = SI.SQLInstanceID
    WHERE ((DPM.DatabaseID =@DatabaseID) OR (@DatabaseID = -1))
 AND ((@UserName IS NULL) OR (DBUser.PrincipalName LIKE @UserName))
AND ((SI.SQLInstanceID = @SQLInstanceID) OR (@SQLInstanceID = 0))
AND (((DPM.IsArchived = 1) AND (@Status = 'Audit')) OR
          ((DPM.IsArchived = 0) AND (@Status = 'Active')) OR
          (@Status = 'Both'))
AND DBUser.PrincipalName NOT IN ('dbo','guest')</CommandText>
      </Query>
      <Fields>
        <Field Name="ServerInstance">
          <DataField>ServerInstance</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DatabaseName">
          <DataField>DatabaseName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DatabaseRoleName">
          <DataField>DatabaseRoleName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DatabaseUserName">
          <DataField>DatabaseUserName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DatabaseUserType">
          <DataField>DatabaseUserType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IsOrphaned">
          <DataField>IsOrphaned</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="IsArchived">
          <DataField>IsArchived</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="DiscoveryOn">
          <DataField>DiscoveryOn</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="LastUpdated">
          <DataField>LastUpdated</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
