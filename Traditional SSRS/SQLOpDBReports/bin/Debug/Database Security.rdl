    <DataSet Name="DatabaseExplicitPermissions">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DatabaseID">
            <Value>=Parameters!DatabaseID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserName">
            <Value>=Parameters!UserName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SQLInstanceID">
            <Value>=Parameters!SQLInstanceID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>   SELECT SI.ServerInstance, Grantee.PrincipalName AS DatabaseUserName, Grantee.PrincipalName AS GranteeName, D.DatabaseName, DP.ObjectName, DP.Access, DP.PermissionName, DP.IsArchived, DP.DiscoveryOn, DP.LastUpdated
     FROM Security.DatabasePermission DP
LEFT JOIN Security.DatabasePrincipal Grantor
       ON DP.GrantorID = Grantor.PrincipalID
LEFT JOIN Security.DatabasePrincipal Grantee
       ON DP.GranteeID = Grantee.PrincipalID
LEFT JOIN dbo.Databases D
       ON DP.DatabaseID = D.DatabaseID
LEFT JOIN dbo.vSQLInstances SI
       ON D.SQLInstanceID = SI.SQLInstanceID
    WHERE ((DP.DatabaseID =@DatabaseID) OR (@DatabaseID = -1))
 AND ((@UserName IS NULL) OR (Grantee.PrincipalName LIKE @UserName))
AND ((SI.SQLInstanceID = @SQLInstanceID) OR (@SQLInstanceID = 0))
AND (((DP.IsArchived = 1) AND (@Status = 'Audit')) OR
          ((DP.IsArchived = 0) AND (@Status = 'Active')) OR
          (@Status = 'Both'))</CommandText>
      </Query>
