  <DataSets>
    <DataSet Name="SQLJobDetail">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@JobStatus">
            <Value>=Parameters!JobStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SQLInstanceID">
            <Value>=Parameters!SQLInstanceID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH LastExecutionPerJob AS (
SELECT SQLJobID, Max(ExecutionDateTime) AS LastExecutionDateTime
  FROM dbo.SQLJobHistory
GROUP BY SQLJobID)
SELECT SI.SQLInstanceID,
        CASE WHEN SI.SQLInstanceName = 'MSSQLServer' THEN 
                     SI.ComputerName
         ELSE
                     SI.ComputerName + '\' +  SI.SQLInstanceName
         END AS ServerInstance, J.SQLJobID, JC.SQLJobCategoryName, J.SQLJobName,  JH.JobStatus, JH.Duration,JH.ExecutionDateTime
  FROM dbo.SQLJobs J
  JOIN dbo.SQLJobCategory JC
    ON J.SQLJobCategoryID = JC.SQLJobCategoryID
  JOIN dbo.SQLJobHistory JH
    ON J.SQLJobID = JH.SQLJobID
  JOIN LastExecutionPerJob LE
    ON JH.SQLJobID = LE.SQLJobID
   AND JH.ExecutionDateTime = LE.LastExecutionDateTime
  JOIN dbo.vSQLInstances SI
    ON J.SQLInstanceID = SI.SQLInstanceID
 WHERE JH.ExecutionDateTime &gt;= DATEADD(Day,-7,GETDATE())
   AND ((JH.JobStatus = @JobStatus) OR (@JobStatus = 'ALL'))
   AND J.SQLJobCategoryID &lt;&gt; 10
   AND ((SI.SQLInstanceID = @SQLInstanceID) OR (@SQLInstanceID = -1))</CommandText>
      </Query>
