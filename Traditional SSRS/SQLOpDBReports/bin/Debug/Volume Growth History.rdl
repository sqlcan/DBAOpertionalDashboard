  <DataSets>
    <DataSet Name="GrowthHistory_Daily">
      <Query>
        <DataSourceName>PrimaryDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DiskVolumeID">
            <Value>=Parameters!DiskVolumeID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH CTE AS (
  SELECT DiskVolumeID, DateCaptured, SpaceUsed_mb, ROW_NUMBER() OVER (ORDER BY DiskVolumeID ASC, DateCaptured ASC) AS RowNum
    FROM dbo.DiskVolumeSpace
   WHERE DiskVolumeID = @DiskVolumeID
),
CTE2 AS (
SELECT DBS1.DiskVolumeID, DBS1.DateCaptured,
       CAST(ROUND((1-((DBS2.SpaceUsed_mb*1.0)/DBS1.SpaceUsed_mb)),4) AS decimal(7,4)) AS GrowthInPercent
  FROM CTE DBS1 
  JOIN CTE DBS2
    ON DBS1.RowNum = DBS2.RowNum+1
   AND DBS1.DiskVolumeID = DBS2.DiskVolumeID)
   --SELECT * FROM CTE2
SELECT CASE WHEN S.ServerName IS NULL THEN SC.SQLClusterName ELSE S.ServerName END AS ComputerName,
       DV.DiskVolumeName,
	   DVS.DateCaptured,
	   SpaceUsed_mb,
	   TotalSpace_mb,
	   GrowthInPercent
  FROM dbo.DiskVolumes DV
  JOIN dbo.DiskVolumeSpace DVS
    ON DV.DiskVolumeID = DVS.DiskVolumeID
  LEFT JOIN CTE2 
    ON DVS.DiskVolumeID = CTE2.DiskVolumeID
   AND DVS.DateCaptured = CTE2.DateCaptured
  LEFT JOIN dbo.Servers S
    ON DV.ServerID = S.ServerID
   AND DV.SQLClusterID IS NULL
  LEFT JOIN dbo.SQLClusters SC
    ON DV.SQLClusterID = SC.SQLClusterID
   AND DV.ServerID IS NULL
WHERE DV.DiskVolumeID = @DiskVolumeID
ORDER BY DVS.DateCaptured,  ComputerName, DiskVolumeName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ComputerName">
          <DataField>ComputerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DiskVolumeName">
          <DataField>DiskVolumeName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DateCaptured">
          <DataField>DateCaptured</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="SpaceUsed_mb">
          <DataField>SpaceUsed_mb</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="TotalSpace_mb">
          <DataField>TotalSpace_mb</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="GrowthInPercent">
          <DataField>GrowthInPercent</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="GrowthHistory_Monthly">
      <Query>
        <DataSourceName>PrimaryDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DiskVolumeID">
            <Value>=Parameters!DiskVolumeID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH CTE AS (
  SELECT DiskVolumeID, YearMonth, SpaceUsed_mb, ROW_NUMBER() OVER (ORDER BY DiskVolumeID ASC, YearMonth ASC) AS RowNum
    FROM History.DiskVolumeSpace
   WHERE DiskVolumeID = @DiskVolumeID
),
CTE2 AS (
SELECT DBS1.DiskVolumeID, DBS1.YearMonth,
       CAST(ROUND((1-((DBS2.SpaceUsed_mb*1.0)/DBS1.SpaceUsed_mb)),4) AS decimal(7,4)) AS GrowthInPercent
  FROM CTE DBS1 
  JOIN CTE DBS2
    ON DBS1.RowNum = DBS2.RowNum+1
   AND DBS1.DiskVolumeID = DBS2.DiskVolumeID)
   --SELECT * FROM CTE2
SELECT CASE WHEN S.ServerName IS NULL THEN SC.SQLClusterName ELSE S.ServerName END AS ComputerName,
       DV.DiskVolumeName,
	   DVS.YearMonth,
	   SpaceUsed_mb,
	   TotalSpace_mb,
	   GrowthInPercent
  FROM dbo.DiskVolumes DV
  JOIN History.DiskVolumeSpace DVS
    ON DV.DiskVolumeID = DVS.DiskVolumeID
  LEFT JOIN CTE2 
    ON DVS.DiskVolumeID = CTE2.DiskVolumeID
   AND DVS.YearMonth = CTE2.YearMonth
  LEFT JOIN dbo.Servers S
    ON DV.ServerID = S.ServerID
   AND DV.SQLClusterID IS NULL
  LEFT JOIN dbo.SQLClusters SC
    ON DV.SQLClusterID = SC.SQLClusterID
   AND DV.ServerID IS NULL
WHERE DV.DiskVolumeID = @DiskVolumeID
ORDER BY DVS.YearMonth,  ComputerName, DiskVolumeName</CommandText>
      </Query>
