    <DataSet Name="DiskVolumeInfo">
      <Query>
        <DataSourceName>PrimaryDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DiskVolumeID">
            <Value>=Parameters!DiskVolumeID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH CTE AS (SELECT CASE WHEN S.ServerName IS NULL THEN SC.SQLClusterName ELSE S.ServerName END AS ComputerName, DV.DiskVolumeID, DV.DiskVolumeName, DV.DiscoveryOn, DV.LastUpdated
FROM DiskVolumes AS DV
LEFT OUTER JOIN Servers AS S ON DV.ServerID = S.ServerID AND DV.SQLClusterID IS NULL
LEFT OUTER JOIN SQLClusters AS SC ON DV.SQLClusterID = SC.SQLClusterID AND DV.ServerID IS NULL)
SELECT *, 'Computer: ' + ComputerName + ' Volume: ' + DiskVolumeName AS ReportTilte
 FROM CTE
WHERE        (DiskVolumeID = @DiskVolumeID)</CommandText>
