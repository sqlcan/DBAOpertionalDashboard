    <DataSet Name="pComputerName">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>SELECT '&lt;ALL&gt;' AS ComputerName
UNION ALL
   SELECT DISTINCT CASE WHEN S.ServerName IS NULL THEN SC.SQLClusterName ELSE S.ServerName END
     FROM dbo.SQLInstances SI
LEFT JOIN dbo.Servers S
       ON SI.ServerID = S.ServerID
	  AND SI.SQLClusterID IS NULL
LEFT JOIN dbo.SQLClusters SC
       ON SI.SQLClusterID = SC.SQLClusterID
	  AND SI.ServerID IS NULL
 ORDER BY ComputerName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ComputerName">
          <DataField>ComputerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="pDiskVolumes">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ComputerName">
            <Value>=Parameters!ComputerName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH CTE AS (SELECT CASE WHEN S.ServerName IS NULL THEN SC.SQLClusterName ELSE S.ServerName END AS ComputerName, DV.DiskVolumeID, DV.DiskVolumeName
FROM DiskVolumes AS DV
LEFT OUTER JOIN Servers AS S ON DV.ServerID = S.ServerID AND DV.SQLClusterID IS NULL
LEFT OUTER JOIN SQLClusters AS SC ON DV.SQLClusterID = SC.SQLClusterID AND DV.ServerID IS NULL)
SELECT ComputerName, DiskVolumeID, DiskVolumeName
 FROM CTE
WHERE ((ComputerName = @ComputerName) OR (@ComputerName = '&lt;ALL&gt;'))
AND DiskVolumeName NOT LIKE '\\?\%'
UNION ALL
SELECT 'Default',0,'&lt;ALL&gt;'</CommandText>
      </Query>
      <Fields>
        <Field Name="ComputerName">
          <DataField>ComputerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DiskVolumeID">
          <DataField>DiskVolumeID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DiskVolumeName">
          <DataField>DiskVolumeName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
