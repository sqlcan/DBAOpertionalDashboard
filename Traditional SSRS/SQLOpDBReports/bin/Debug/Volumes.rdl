  <DataSets>
    <DataSet Name="DiskVolumeDetails">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DiskVolume">
            <Value>=Parameters!DiskVolume.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ComputerName">
            <Value>=Parameters!ComputerName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- Is Monitored check is not needed, as it is applied in the view.

SELECT *
  FROM dbo.vDiskVolumeSpace
    WHERE ((DiskVolumeID = @DiskVolume) OR (@DiskVolume = 0))
AND DiskVolumeName NOT LIKE '\\?\%'
AND ((@ComputerName = '&lt;ALL&gt;') OR (ComputerName = @ComputerName))
 ORDER BY DaysUntil_OutOfSpace, ComputerName, DiskVolumeName</CommandText>
      </Query>
      <Fields>
