  <DataSets>
    <DataSet Name="DiskVolumeDetails">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DiskVolume">
            <Value>=Parameters!DiskVolume.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH CTE
AS (SELECT DiskVolumeID,
           SpaceUsed_mb AS LastSpaceUsed_mb,
		   TotalSpace_mb AS LastTotalSpace_mb
      FROM dbo.DiskVolumeSpace DVS1
     WHERE SpaceHistoryID = (SELECT MAX(SpaceHistoryID) FROM dbo.DiskVolumeSpace DVS2 WHERE DVS1.DiskVolumeID = DVS2.DiskVolumeID))

   SELECT CASE WHEN S.ServerName IS NULL THEN SC.SQLClusterName ELSE S.ServerName END AS ComputerName,
          DV.DiskVolumeID, 
          DV.DiskVolumeName,
	      DVSA.SpaceUsed_mb AS AvgSpaceUsed_mb,
	      DVSA.TotalSpace_mb AS AvgTotalSpace_mb,
		  LastSpaceUsed_mb,
		  LastTotalSpace_mb,
		  (LastSpaceUsed_mb + 1.0)/(LastTotalSpace_mb + 1.0) AS SpaceUsedInPercent,
	      AvgGrowthInPercent,
          CASE WHEN AvgGrowthInPercent &gt; 0 AND LOG(1+AvgGrowthInPercent) &gt; 0.0 THEN
              LOG((LastTotalSpace_mb + 1.0)/(LastSpaceUsed_mb + 1.0)) / LOG(1+AvgGrowthInPercent)
	      ELSE
   	          9999999
	      END AS DaysUntilOutOfSpace
     FROM dbo.DiskVolumes DV
     JOIN dbo.vwDiskVolumeSpaceAverage DVSA
       ON DV.DiskVolumeID = DVSA.DiskVolumeID
     JOIN CTE DVSL
       ON DV.DiskVolumeID = DVSL.DiskVolumeID
LEFT JOIN dbo.Servers S
       ON DV.ServerID = S.ServerID
      AND DV.SQLClusterID IS NULL
LEFT JOIN dbo.SQLClusters SC
       ON DV.SQLClusterID = SC.SQLClusterID
      AND DV.ServerID IS NULL
    WHERE ((DV.DiskVolumeID = @DiskVolume) OR (@DiskVolume = 0))
AND DiskVolumeName NOT LIKE '\\?\%'
 ORDER BY DaysUntilOutOfSpace, ComputerName, DiskVolumeName</CommandText>
      </Query>
      <Fields>
        <Field Name="ComputerName">
          <DataField>ComputerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DiskVolumeID">
          <DataField>DiskVolumeID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DiskVolumeName">
          <DataField>DiskVolumeName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AvgSpaceUsed_mb">
          <DataField>AvgSpaceUsed_mb</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="AvgTotalSpace_mb">
          <DataField>AvgTotalSpace_mb</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="AvgGrowthInPercent">
          <DataField>AvgGrowthInPercent</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="LastSpaceUsed_mb">
          <DataField>LastSpaceUsed_mb</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="SpaceUsedInPercent">
          <DataField>SpaceUsedInPercent</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="LastTotalSpace_mb">
          <DataField>LastTotalSpace_mb</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="DaysUntilOutOfSpace">
          <DataField>DaysUntilOutOfSpace</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="LastFreeSpace_mb">
          <Value>=Fields!LastTotalSpace_mb.Value-Fields!LastSpaceUsed_mb.Value</Value>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="pComputerName">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandText>SELECT '&lt;ALL&gt;' AS ComputerName
UNION ALL
   SELECT DISTINCT CASE WHEN S.ServerName IS NULL THEN SC.SQLClusterName ELSE S.ServerName END
     FROM dbo.SQLInstances SI
LEFT JOIN dbo.Servers S
       ON SI.ServerID = S.ServerID
	  AND SI.SQLClusterID IS NULL
LEFT JOIN dbo.SQLClusters SC
       ON SI.SQLClusterID = SC.SQLClusterID
	  AND SI.ServerID IS NULL
 ORDER BY ComputerName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ComputerName">
          <DataField>ComputerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="pDiskVolumes">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ComputerName">
            <Value>=Parameters!ComputerName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH CTE AS (SELECT CASE WHEN S.ServerName IS NULL THEN SC.SQLClusterName ELSE S.ServerName END AS ComputerName, DV.DiskVolumeID, DV.DiskVolumeName
FROM DiskVolumes AS DV
LEFT OUTER JOIN Servers AS S ON DV.ServerID = S.ServerID AND DV.SQLClusterID IS NULL
LEFT OUTER JOIN SQLClusters AS SC ON DV.SQLClusterID = SC.SQLClusterID AND DV.ServerID IS NULL)
SELECT ComputerName, DiskVolumeID, DiskVolumeName
 FROM CTE
WHERE ((ComputerName = @ComputerName) OR (@ComputerName = '&lt;ALL&gt;'))
AND DiskVolumeName NOT LIKE '\\?\%'
UNION ALL
SELECT 'Default',0,'&lt;ALL&gt;'</CommandText>
      </Query>
      <Fields>
        <Field Name="ComputerName">
          <DataField>ComputerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DiskVolumeID">
          <DataField>DiskVolumeID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DiskVolumeName">
          <DataField>DiskVolumeName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
