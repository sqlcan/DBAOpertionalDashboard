  <DataSets>
    <DataSet Name="SQLServices">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ServerName">
            <Value>=Parameters!ServerName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@ServiceType">
            <Value>=Parameters!ServiceType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartMode">
            <Value>=Parameters!StartMode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>  SELECT S.ServerName, SS.DisplayName, SS.FilePath, SS.ServiceType, SS.StartMode, SS.Status,SS.ServiceAccount, SS.ServiceBuild, SS.DiscoveryOn, SS.LastUpdated
    FROM dbo.SQLService SS
	JOIN dbo.Servers S
	  ON SS.ServerID = S.ServerID
WHERE SS.LastUpdated &gt;= DATEADD(DAY,-1,GETDATE())
    AND  ((S.ServerName = @ServerName) OR (@ServerName = 'ALL'))
     AND ((SS.ServiceType = @ServiceType) OR (@ServiceType = 'ALL'))
    AND ((SS.StartMode = @StartMode) OR (@StartMode = 'ALL'))
    AND ((SS.Status = @Status) OR ((SS.Status IS NULL) AND (@Status='Unknown')) OR (@Status = 'ALL'))</CommandText>
      </Query>
      <Fields>
        <Field Name="ServerName">
          <DataField>ServerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DisplayName">
          <DataField>DisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilePath">
          <DataField>FilePath</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ServiceType">
