        <Field Name="LastUpdated">
          <DataField>LastUpdated</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT SI.SQLInstanceID,
          CASE WHEN (S.ServerID IS NULL) THEN SC.SQLClusterName ELSE S.ServerName END AS ServerVCOName,
		  SI.SQLInstanceName,
		  SI.SQLInstanceEnviornment,
		  SV.SQLVersion,
		  SV.SQLVersionSupportEndDate,
		  SI.SQLInstanceBuild,
		  SI.SQLInstanceEdition,
		  SI.DiscoveryOn,
		  SI.LastUpdated
     FROM dbo.SQLInstances SI
	 JOIN dbo.SQLVersions SV
	   ON SI.SQLInstanceVersionID = SV.SQLVersionID
LEFT JOIN dbo.Servers S
       ON SI.ServerID = S.ServerID
      AND SI.SQLClusterID IS NULL
LEFT JOIN dbo.SQLClusters SC
       ON SI.SQLClusterID = SC.SQLClusterID
	  AND SI.ServerID IS NULL</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
