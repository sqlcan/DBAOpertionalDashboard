  <DataSets>
    <DataSet Name="report_EPM_ObjectCount_By_Category_Policy_Result">
      <Query>
        <DataSourceName>dsEPM</DataSourceName>
        <CommandText>SELECT   REPLACE(REPLACE(PHS.CategoryName,'AHS Best Practices: ',''),'Microsoft Best Practices: ','') AS CategoryName
       , PSN.Policy_ShortName AS PolicyName
       , PHS.PolicyResult
       , COUNT(*) AS ObjCount
  FROM policy.PolicyHistorySummary PHS
  JOIN dbo.PolicyShortName PSN
    ON PHS.policy_id = PSN.policy_id
 WHERE NOT EXISTS (SELECT *
                     FROM DBA_Resource.EPM.PolicyExclusions PE
                    WHERE PHS.EvaluatedServer LIKE PE.EvaluatedServer
                      AND PHS.ObjectName LIKE PE.ObjectName
                      AND PHS.policy_id = PE.Policy_ID)
GROUP BY PHS.CategoryName, PSN.Policy_ShortName, PHS.PolicyResult
ORDER BY 1,2,3</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CategoryName">
          <DataField>CategoryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PolicyName">
          <DataField>PolicyName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PolicyResult">
          <DataField>PolicyResult</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ObjCount">
          <DataField>ObjCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
