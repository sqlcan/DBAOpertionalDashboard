  <DataSets>
    <DataSet Name="dsMain">
      <Query>
        <DataSourceName>dsMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SQLEnvironment">
            <Value>=Parameters!SQLEnvironment.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SQLVersion">
            <Value>=Parameters!SQLVersion.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH
DBList AS
(
       SELECT   LOWER(SQLServerName) AS SQLServerName
              , DBName
                 , LastUsed
         FROM DatabaseUsageStats.dbo.DBUsageStats

       UNION ALL

       SELECT   LOWER(SQLServerName) AS SQLServerName
              , DBName
                 , MAX(DateLogged)
         FROM DatabaseUsageStats.dbo.SQL200DBUsageDetails
  GROUP BY SQLServerName, DBName
),
CMDBInstanceDBList AS
(
          SELECT CASE WHEN SI.ServerID IS NULL THEN
                             SC.SQLClusterName
                       ELSE
                             S.ServerName
                       END AS SQLServerName
                       , SI.SQLInstanceName
                       , CASE WHEN SI.ServerID IS NULL THEN
                                  OS2.OperatingSystemName
                           ELSE
                                  OS1.OperatingSystemName
                           END AS OperatingSystemName
                       , SV.SQLVersion
                       , SI.SQLInstanceBuild
                       , SI.SQLInstanceEdition
                       , SI.SQLInstanceType
                       , SI.SQLInstanceEnviornment
					   , D.DatabaseName
					   , SI.LastUpdated
              FROM dbo.SQLInstances SI
              JOIN dbo.SQLVersions SV
                 ON SI.SQLInstanceVersionID = SV.SQLVersionID
       LEFT JOIN dbo.Databases D
	             ON SI.SQLInstanceID = D.SQLInstanceID
       LEFT JOIN dbo.Servers S
                 ON SI.ServerID = S.ServerID
                AND SI.SQLClusterID IS NULL 
       LEFT JOIN dbo.SQLClusters SC
                 ON SI.SQLClusterID = SC.SQLClusterID
                AND SI.ServerID IS NULL
       LEFT JOIN dbo.SQLClusterNodes SCN
                 ON SC.SQLClusterID = SCN.SQLClusterID
                AND SCN.IsActiveNode = 1
       LEFT JOIN dbo.Servers SCNS
                 ON SCN.SQLNodeID = SCNS.ServerID
       LEFT JOIN dbo.OperatingSystems OS1
                 ON S.OperatingSystemID = OS1.OperatingSystemID
       LEFT JOIN dbo.OperatingSystems OS2
                 ON SCNS.OperatingSystemID = OS2.OperatingSystemID
)
   SELECT   I.*
             , DB.LastUsed
     FROM CMDBInstanceDBList I
LEFT JOIN DBList DB
       ON DB.SQLServerName = I.SQLServerName + '\' + I.SQLInstanceName
	  AND DB.DBName = I.DatabaseName
    WHERE LastUpdated &gt;= DATEADD(DAY,-60,GETDATE())
	  AND I.SQLInstanceEnviornment LIKE @SQLEnvironment
	  AND I.SQLVersion LIKE @SQLVersion</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
