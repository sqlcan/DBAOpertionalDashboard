  <DataSets>
    <DataSet Name="DatabaseDetails">
      <Query>
        <DataSourceName>PrimaryDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DatabaseID">
            <Value>=Parameters!DatabaseID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FileType">
            <Value>=Parameters!FileType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH CTE AS (
  SELECT DatabaseID, DateCaptured, FileSize_mb, ROW_NUMBER() OVER (ORDER BY DatabaseID ASC, DateCaptured ASC) AS RowNum
    FROM dbo.DatabaseSize
   WHERE DatabaseID = @DatabaseID
     AND FileType = @FileType
),
CTE2 AS (
SELECT DBS1.DatabaseID, DBS1.DateCaptured,
       CAST(ROUND((1-((DBS2.FileSize_mb*1.0)/DBS1.FileSize_mb)),4) AS decimal(7,4)) AS File_GrowthInPercent
  FROM CTE DBS1 
  JOIN CTE DBS2
    ON DBS1.RowNum = DBS2.RowNum+1
   AND DBS1.DatabaseID = DBS2.DatabaseID)
   --SELECT * FROM CTE2
SELECT D.DatabaseID,
	   DS.DateCaptured,
	   FileSize_mb,
	   File_GrowthInPercent AS AvgGrowthInPercent
  FROM dbo.Databases D
  JOIN dbo.DatabaseSize DS
    ON D.DatabaseID = DS.DatabaseID
  LEFT JOIN CTE2 
    ON DS.DatabaseID = CTE2.DatabaseID
   AND DS.DateCaptured = CTE2.DateCaptured
WHERE D.IsMonitored = 1
  AND D.DatabaseID = @DatabaseID
  AND DS.FileType = @FileType
ORDER BY DS.DateCaptured</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="DatabaseID">
          <DataField>DatabaseID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FileSize_mb">
          <DataField>FileSize_mb</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="DateCaptured">
          <DataField>DateCaptured</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="AvgGrowthInPercent">
          <DataField>AvgGrowthInPercent</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DatabaseSpaceHistory">
      <Query>
        <DataSourceName>PrimaryDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DatabaseID">
            <Value>=Parameters!DatabaseID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FileType">
            <Value>=Parameters!FileType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH CTE AS (SELECT     DatabaseID, YearMonth, FileSize_mb, ROW_NUMBER() OVER (ORDER BY DatabaseID ASC, YearMonth ASC) AS RowNum
FROM         History.DatabaseSize
WHERE     DatabaseID = @DatabaseID AND FileType = @FileType), CTE2 AS
    (SELECT     DBS1.DatabaseID, DBS1.YearMonth, CAST(ROUND((1 - ((DBS2.FileSize_mb * 1.0) / DBS1.FileSize_mb)), 4) AS decimal(7, 4)) AS File_GrowthInPercent
      FROM          CTE DBS1 JOIN
                             CTE DBS2 ON DBS1.RowNum = DBS2.RowNum + 1 AND DBS1.DatabaseID = DBS2.DatabaseID)
SELECT DS.YearMonth, FileSize_mb, File_GrowthInPercent AS AvgGrowthInPercent
     FROM         dbo.Databases D JOIN
                            History.DatabaseSize DS ON D .DatabaseID = DS.DatabaseID LEFT JOIN
                            CTE2 ON DS.DatabaseID = CTE2.DatabaseID AND DS.YearMonth = CTE2.YearMonth
     WHERE     D .IsMonitored = 1 AND D .DatabaseID = @DatabaseID AND FileType = @FileType
     ORDER BY DS.YearMonth</CommandText>
      </Query>
      <Fields>
        <Field Name="FileSize_mb">
          <DataField>FileSize_mb</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="YearMonth">
          <DataField>YearMonth</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AvgGrowthInPercent">
          <DataField>AvgGrowthInPercent</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DatabaseInfo">
      <Query>
        <DataSourceName>PrimaryDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DatabaseID">
            <Value>=Parameters!DatabaseID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     CASE WHEN S.ServerName IS NULL THEN SC.SQLClusterName ELSE S.ServerName END AS ServerVCOName, D.DatabaseID, D.DatabaseName, D.DiscoveryOn, 
                      D.LastUpdated
FROM         Databases AS D INNER JOIN
                      SQLInstances AS SI ON SI.SQLInstanceID = D.SQLInstanceID LEFT OUTER JOIN
                      Servers AS S ON SI.ServerID = S.ServerID AND SI.SQLClusterID IS NULL LEFT OUTER JOIN
                      SQLClusters AS SC ON SI.SQLClusterID = SC.SQLClusterID AND SI.ServerID IS NULL
WHERE     (D.IsMonitored = 1) AND (D.DatabaseID = @DatabaseID)</CommandText>
        <rd:DesignerState><QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational"><SelectedColumns><ColumnExpression ColumnOwner="DiskVolumes" ColumnName="DiskVolumeID" /><ColumnExpression ColumnOwner="DiskVolumes" ColumnName="DiskVolumeName" /><ColumnExpression ColumnOwner="DiskVolumes" ColumnName="DiscoveryOn" /><ColumnExpression ColumnOwner="DiskVolumes" ColumnName="LastUpdated" /></SelectedColumns></QueryDefinition></rd:DesignerState>
      </Query>
      <Fields>
        <Field Name="DatabaseID">
          <DataField>DatabaseID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ServerVCOName">
          <DataField>ServerVCOName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DatabaseName">
          <DataField>DatabaseName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DiscoveryOn">
