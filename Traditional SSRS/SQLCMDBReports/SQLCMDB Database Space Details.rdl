  <DataSources>
    <DataSource Name="PrimaryDS">
      <DataSourceReference>dsMain</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>c33de668-9b0d-410d-ab2a-23ea312f82b0</rd:DataSourceID>
