  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="EvaluatedServer">
          <DataField>EvaluatedServer</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DatabaseName">
          <DataField>DatabaseName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsEPM</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>WITH CTE AS (SELECT EvaluatedServer, EvaluatedPolicy, EvaluatedObject, MAX(EvaluationDateTime) AS LastEval
  FROM policy.PolicyHistoryDetail PHD
 WHERE policy_id = 1018
GROUP BY EvaluatedServer, EvaluatedPolicy, EvaluatedObject)
SELECT PHD.EvaluatedServer,
       	   SUBSTRING(PHD.EvaluatedObject,
	             LEN(PHD.EvaluatedObject) - CHARINDEX('\',REVERSE(PHD.EvaluatedObject)) + 2,
				 LEN(PHD.EvaluatedObject)) AS DatabaseName
  FROM policy.PolicyHistoryDetail PHD
  JOIN CTE 
    ON PHD.EvaluatedServer = CTE.EvaluatedServer
   AND PHD.EvaluatedPolicy = CTE.EvaluatedPolicy
   AND PHD.EvaluatedObject = CTE.EvaluatedObject
   AND PHD.EvaluationDateTime = CTE.LastEval
 WHERE PolicyResult = 'Fail'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
